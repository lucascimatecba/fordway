{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/angular"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "VITE_FIREBASE_API_KEY": "@fordway-firebase-api-key",
    "VITE_FIREBASE_AUTH_DOMAIN": "@fordway-firebase-auth-domain",
    "VITE_FIREBASE_PROJECT_ID": "@fordway-firebase-project-id",
    "VITE_FIREBASE_STORAGE_BUCKET": "@fordway-firebase-storage-bucket",
    "VITE_FIREBASE_MESSAGING_SENDER_ID": "@fordway-firebase-messaging-sender-id",
    "VITE_FIREBASE_APP_ID": "@fordway-firebase-app-id"
  }
}